{
  "name": "cyberaroom-prettierrc",
  "version": "2.0.2",
  "description": "quick. easy. readable.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "github",
    "url": "https://github.com/cyberaroom/prettierrc.git"
  },
  "workspaces": [
    "src/*",
    "playground/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "pnpm -r test",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "files": [
    ".prettierignore",
    "src/index.js",
    "src/index.d.ts"
  ],
  "keywords": [
    "prettier",
    "prettierrc",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-organize-imports",
    "prettier-plugin-organize-attributes",
    "prettier-plugin-jsdoc"
  ],
  "author": "oldiberezkoo",
  "license": "MIT",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "@swc/core": "^1.13.2"
  },
  "dependencies": {
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
}
